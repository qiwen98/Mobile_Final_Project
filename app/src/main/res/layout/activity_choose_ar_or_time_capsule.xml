<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.logintesting.Choose_ar_or_time_capsule">

    <ImageButton
        android:id="@+id/TimeCapsuleBut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="167dp"
        android:layout_marginTop="312dp"
        android:layout_marginEnd="196dp"
        android:layout_marginBottom="79dp"
        app:layout_constraintBottom_toTopOf="@+id/ArCapsuleBut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ictime" />

    <ImageButton
        android:id="@+id/ArCapsuleBut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="168dp"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="196dp"
        android:layout_marginBottom="254dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TimeCapsuleBut"
        app:srcCompat="@drawable/ic_ar" />

</androidx.constraintlayout.widget.ConstraintLayout>